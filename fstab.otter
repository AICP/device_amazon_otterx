# Android fstab file.
#<src>                                            <mnt_point>        <type>    <mnt_flags and options>                            <fs_mgr_flags>
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK
/dev/block/mmcblk0p9                              /system            ext4      rw,nosuid,nodev,noatime,errors=continue,discard    wait
/dev/block/mmcblk0p10                             /data              ext4      rw,nosuid,nodev,noatime,errors=continue,discard    wait,check,encryptable=footer
/dev/block/mmcblk0p11                             /cache             ext4      rw,nosuid,nodev,noatime,errors=continue,discard    wait,check
/devices/platform/omap/omap_hsmmc.1/mmc_host/mmc0 /storage/sdcard0   vfat      defaults                                           voldmanaged=sdcard:12
/dev/block/mmcblk0p7                              /boot              emmc      defaults                                           defaults
/dev/block/mmcblk0p5                              /recovery          emmc      defaults                                           defaults
/dev/block/mmcblk0p2                              /bootloader        emmc      defaults                                           defaults

